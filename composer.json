{
    "name": "hlogeon/scms",
    "description": "Very simple CMS for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Degtyaruk",
            "email": "hlogeon1@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "5.*",
        "illuminate/routing": "5.*",
        "illuminate/html": "~5.0",
        "bican/roles": "1.7.*",
        "fzaninotto/faker": "^1.5",
        "baum/baum": "~1.1",
        "kris/laravel-form-builder": "1.5.*",
        "sleeping-owl/admin": "2.*",
        "cviebrock/eloquent-sluggable": ">=3.0.0-alpha"
    },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.3-dev"
    }
  },
  "require-dev": {
      "phpunit/phpunit": "~4.0",
      "phpspec/phpspec": "~2.1",
      "barryvdh/laravel-debugbar": "^2.0",
      "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "classmap": [
      "src/Http/Controllers/"
    ],
    "psr-4": {
      "Hlogeon\\Scms\\": "scms/src/"
    }
  }

}
